        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.2",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5"
