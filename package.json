{
    "name": "bing-at-work-extension",
    "displayName": "Bing At Work",
    "version": "1.0.0",
    "description": "Browser extension provides different experience of Bing At Work (Microsoft Search in Bing).",
    "keywords": [
        "Bing At Work",
        "Microsoft Search"
    ],
    "homepage": "https://github.com/anders-liu/bing-at-work-extension",
    "readme": "https://github.com/anders-liu/bing-at-work-extension",
    "bugs": {
        "url": "https://github.com/anders-liu/bing-at-work-extension/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Anders Liu"
    },
    "scripts": {
        "build-prod": "cross-env TS_NODE_PROJECT=\"build/tsconfig.webpack.json\" webpack --config build/webpack.config.ts --mode production",
        "build-dev": "cross-env TS_NODE_PROJECT=\"build/tsconfig.webpack.json\" webpack --config build/webpack.config.ts --mode development"
    },
    "devDependencies": {
        "@types/html-webpack-plugin": "^3.2.2",
        "@types/node": "^13.7.4",
        "@types/react": "^16.9.22",
        "@types/react-dom": "^16.9.5",
        "@types/webpack": "^4.41.6",
        "cross-env": "^7.0.0",
